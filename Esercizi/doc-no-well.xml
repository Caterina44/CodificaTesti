<?xml version="1.1" encoding="UTF-8"?>
<docu>
    <element>

    </element>
    <nowell>
        <over> <!-- overlap: il tag di chiusura di <over> (elemento figlio)
                si trova all'esterno del tag di chiusura di <element> (elemento padre): 
                lo sposto prima di </noWell> -->
        </over>
    </noWell>

    <newElement>
    questo è un nuovo elemento child
    <!-- <newElement> è un elemento child rispetto a <docu> 
        e un elemento sibling rispetto a <element> e <nowell> -->
    </newElement>

    <newSibling>
    questo è un nuovo elemento sibling
    <!-- <newSibling> è un nuovo elemento, 
        sullo stesso livello di <newElement>, <noWell> e <element> -->
    </newSibling
</docu>